
{
  "compilerOptions": {
    "target": "es2015",
    "jsx": "react",
    "noEmit": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "allowSyntheticDefaultImports": false,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "allowJs": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "baseUrl": "."
  },
  "exclude": ["node_modules"]
}